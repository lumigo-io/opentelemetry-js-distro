{
  "name": "@lumigo/opentelemetry",
  "version": "1.18.0",
  "description": "Lumigo wrapper to trace distributed architecture",
  "main": "dist/src/wrapper.js",
  "private": false,
  "types": "dist/src/wrapper.d.ts",
  "scripts": {
    "build": "rm -rf dist wrapper.tgz && tsc --build --force && tracer=\"$(npm pack)\" && mv \"$tracer\" wrapper.tgz",
    "prepublishOnly": "npm run prettier:fix && npm run lint && npm run test:unit",
    "lint": "eslint \"src/**/*.{ts,tsx,js}\"",
    "install-component": "bash -c 'source scripts/install_component.sh && install_component_dependencies'",
    "install-integration": "bash -c 'source scripts/install_component.sh && install_integration_dependencies'",
    "prettier:ci": "prettier --list-different \"src/**/*.[jt]s\" --parser typescript",
    "prettier:fix": "prettier --write \"./src/**/*.[jt]s\" --parser typescript",
    "semantic-release": "semantic-release",
    "test:unit": "npm run build && NODE_OPTIONS=--unhandled-rejections=strict jest -c jest.unit.config.js --detectOpenHandles --runInBand --forceExit --silent",
    "test:component": "npm run build && npm run install-component && jest -c jest.component.config.js --detectOpenHandles --runInBand --forceExit",
    "test:integration:version": "npm run build && npm run install-integration && jest -c jest.integration.config.js --detectOpenHandles --runInBand --forceExit --silent=false",
    "local:test:integration:version": "npm run build && npm run install-integration && jest -c jest.integration.config.js --detectOpenHandles --forceExit --silent=false",
    "tsc": "tsc"
  },
  "repository": "git@github.com:lumigo-io/opentelemetry-js-distro.git",
  "author": "Lumigo",
  "dependencies": {
    "@lumigo/node-core": "^1.13.0",
    "@opentelemetry/api": "1.4.0",
    "@opentelemetry/core": "1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.35.1",
    "@opentelemetry/instrumentation": "0.35.1",
    "@opentelemetry/instrumentation-express": "0.32.0",
    "@opentelemetry/instrumentation-http": "0.35.1",
    "@opentelemetry/instrumentation-mongodb": "0.34.0",
    "@opentelemetry/resource-detector-aws": "1.2.1",
    "@opentelemetry/resources": "1.9.0",
    "@opentelemetry/sdk-trace-base": "1.9.0",
    "@opentelemetry/sdk-trace-node": "1.9.0",
    "@opentelemetry/semantic-conventions": "1.9.0",
    "opentelemetry-instrumentation-express": "0.34.0",
    "shimmer": "^1.2.1"
  },
  "homepage": "https://github.com/lumigo-io/opentelemetry-js-distro.git",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.16.7",
    "@jest/globals": "^29.4.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.5",
    "@types/express": "^4.17.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "aws-config": "^1.4.0",
    "aws-sdk": "^2.1066.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.1",
    "bufferutil": "^4.0.1",
    "chai": "^4.3.6",
    "console-stamp": "^3.0.6",
    "cross-env": "^6.0.0",
    "encoding": "^0.1.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^29.4.0",
    "jest-chain": "^1.1.6",
    "jest-json": "^2.0.0",
    "jest-summarizing-reporter": "^1.1.4",
    "log-timestamp": "^0.3.0",
    "mock-fs": "^5.2.0",
    "multiline": "^2.0.0",
    "node-notifier": "^10.0.0",
    "npm-install-peers": "^1.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "semver": "^7.3.7",
    "tmp": "^0.2.1",
    "tree-kill": "^1.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.4",
    "utf-8-validate": "^5.0.2",
    "wait-on": "^6.0.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.10.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
